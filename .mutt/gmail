set imap_user = ''
set imap_pass = ''
set from= $imap_user
set use_from=yes
set folder = imaps://imap.gmail.com:993
set spoolfile = "+INBOX"
set mail_check = 100
set postponed="+[gmail]/Drafts"
set smtp_url = "smtp://$imap_user@smtp.gmail.com:587"
set smtp_pass = $imap_pass
set move = no
set imap_keepalive = 900
set record="+Sent"
