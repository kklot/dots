set imap_user = ''
set imap_pass = ''
set from= $imap_user
set use_from=yes
set folder = imaps://outlook.office365.com:993
set spoolfile = "+INBOX"
set postponed="+[hotmail]/Drafts"
set mail_check = 100
set smtp_url = "smtp://$imap_user@smtp.office365.com:587"
set smtp_pass = $imap_pass
set move = no
set imap_keepalive = 900
set record="+Sent"
